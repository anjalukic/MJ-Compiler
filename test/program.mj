program MJProgram
//upis: 5 2
//ispis:  21 20
{
	int mul(int a, int b){
		return a * b;
	}
	
	int sum(int a, int b){
		return a + b;
	}
	
	int sub(int a, int b){
		return a - b;
	}
	
	int div(int a, int b){
		return a/b;
	}
	
	void main() int a, b, razlikaKvadrata, proizvodProizvodaIKolicnika;
	{
		read(a);
		read(b);
		razlikaKvadrata = mul(sub(a,b) , sum(a,b));
		proizvodProizvodaIKolicnika = mul(mul(a,b), div(a,b));
		
		print(razlikaKvadrata);
		print(proizvodProizvodaIKolicnika);
	}
}